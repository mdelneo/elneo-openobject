<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_transfert_move_wizard" model="ir.ui.view">
            <field name="name">Transfert move</field>
            <field name="model">transfert.move.wizard</field>
            <field name="arch" type="xml">
                <form string="Transfert move" version="7">
                	<group>
	                	<field name="product_id" readonly="1" />
	                	<field name="quantity" />
	                	<field name="move_id" invisible="1" />
                	</group>
                    <footer>
                        <button name="do_transfert" string="_Apply" type="object" class="oe_highlight"/>
                        or
                        <button string="_Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>
