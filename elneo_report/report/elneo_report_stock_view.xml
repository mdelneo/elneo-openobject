<?xml version="1.0"?>
<openerp>
    <data>
    	<report 
            string="Picking list" 
            id="report_picking_list_elneo" 
            model="stock.picking" 
            report_type="qweb-pdf"
            name="elneo_report.stock_picking_list_document" 
            file="elneo_report.stock_picking_list_document"
        />
        
        <template id="stock_picking_list">
		    <t t-call="report.html_container">
		        <t t-foreach="doc_ids" t-as="doc_id">
		            <t t-raw="translate_doc(doc_id, doc_model, 'lang', 'elneo_report.stock_picking_list_document')"/>
		        </t>
		    </t>
		</template>
		
		<template id="stock_picking_list_document">
		    <t t-call="report.internal_layout">
		        <div class="page">
		            <div class="oe_structure"/>
		            <h1>PICKING LIST</h1>
				</div>
		    </t>
		</template>
				
    </data>
</openerp>
